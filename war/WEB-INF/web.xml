	</servlet>
	<servlet-mapping>
		<servlet-name>MerchantServlet</servlet-name>
		<url-pattern>/domerchant</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>around.jsp</welcome-file>
	</welcome-file-list>
</web-app>
