		<servlet-name>LoyaltyPromotionServlet</servlet-name>
		<url-pattern>/doloyaltypromotion</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PromotionServlet</servlet-name>
		<servlet-class>gozolabs.com.PromotionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
