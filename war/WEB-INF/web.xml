	<servlet-mapping>
		<servlet-name>SendServlet</servlet-name>
		<url-pattern>/send</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>CategoryServlet</servlet-name>
		<servlet-class>gozolabs.com.CategoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CategoryServlet</servlet-name>
		<url-pattern>/docategory</url-pattern>
	</servlet-mapping>
	
	<servlet>
